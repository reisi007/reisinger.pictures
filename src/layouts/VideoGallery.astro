---
import type {CollectionEntry} from 'astro:content';

import Video from './Video.astro';

interface Props { videos: CollectionEntry<'videos'>[] }

const {videos} = Astro.props;
---
<div class="m-4 grid gap-y-4 md:grid-cols-2 md:gap-x-4 xl:grid-cols-3">
  {videos.map(e =>
          <Video {...e}/>)}
</div>
