{
  "name": "reisinger.pictures",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "editor:css": "tailwindcss -i ./editor.tw.css -o ./editor.css --watch",
    "prebuild": "node add-metadata.mjs",
    "postbuild": "node clean_output.mjs && node check_links.mjs",
    "preview": "astro preview",
    "pretest": "npx playwright install",
    "test": "playwright test",
    "test:local": "playwright test",
    "test:prod": "cross-env BASE_URL=https://reisinger.pictures playwright test",
    "lint": "eslint src *.mjs",
    "lint:fix": "eslint src *.mjs --fix",
    "publish": "npm run build && sync.bat"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.3",
    "@astrojs/sitemap": "^3.4.2",
    "@tailwindcss/cli": "^4.1.11",
    "astro": "^5.13.3",
    "dayjs": "^1.11.13",
    "photoswipe": "^5.4.4",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@iconify-json/material-symbols": "^1.2.30",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify/tailwind4": "^1.0.6",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.38.1-alpha.8",
    "@typescript-eslint/parser": "^8.38.1-alpha.8",
    "daisyui": "^5.0.50",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-regexp": "^2.9.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "exifreader": "^4.31.1",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "typescript": "5.9.2"
  }
}
